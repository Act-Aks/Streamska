{
    "name": "Streamska",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "typecheck": "tsc --noEmit",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prettier": "prettier --check --ignore-unknown .",
        "prettier:fix": "prettier --write --ignore-unknown .",
        "start": "react-native start",
        "test": "jest",
        "preinstall": "npx only-allow yarn",
        "up:i": "yarn upgrade-interactive",
        "up:self": "yarn set version latest && yarn",
        "cq": "yarn prettier -c . && yarn lint && yarn typecheck"
    },
    "dependencies": {
        "react": "19.1.0",
        "react-native": "0.78.2"
    },
    "devDependencies": {
        "@act-aks/eslint-config": "^1.0.2",
        "@babel/core": "^7.26.10",
        "@babel/preset-env": "^7.26.9",
        "@babel/runtime": "^7.27.0",
        "@commitlint/cli": "^19.8.0",
        "@commitlint/config-conventional": "^19.8.0",
        "@eslint/js": "^9.24.0",
        "@react-native-community/cli": "18.0.0",
        "@react-native-community/cli-platform-android": "15.1.3",
        "@react-native-community/cli-platform-ios": "15.1.3",
        "@react-native/babel-preset": "0.78.2",
        "@react-native/eslint-config": "0.78.2",
        "@react-native/metro-config": "0.78.2",
        "@react-native/typescript-config": "0.78.2",
        "@testing-library/jest-native": "^5.4.3",
        "@testing-library/react": "^16.3.0",
        "@testing-library/react-native": "^13.2.0",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/jest": "^29.5.14",
        "@types/react": "^19.1.0",
        "@types/react-test-renderer": "^19.1.0",
        "babel-plugin-module-resolver": "^5.0.2",
        "eslint": "^9.24.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "npm:^5.2.0",
        "eslint-plugin-react-native": "npm:^5.0.0",
        "eslint-plugin-testing-library": "^7.1.1",
        "globals": "^16.0.0",
        "jest": "^29.7.0",
        "lefthook": "^1.11.6",
        "prettier": "3.5.3",
        "prettier-plugin-sort-json": "^4.1.1",
        "react-test-renderer": "19.1.0",
        "typescript": "5.8.3",
        "typescript-eslint": "^8.29.0"
    },
    "engines": {
        "node": ">=22",
        "yarn": ">=4.8.1"
    },
    "packageManager": "yarn@4.8.1"
}
