{
    "name": "Streamska",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "typecheck": "tsc --noEmit",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prettier": "prettier --check --ignore-unknown .",
        "prettier:fix": "prettier --write --ignore-unknown .",
        "start": "react-native start",
        "test": "jest",
        "preinstall": "npx only-allow yarn",
        "up:i": "yarn upgrade-interactive",
        "up:self": "yarn set version latest && yarn",
        "cq": "yarn prettier && yarn lint && yarn typecheck && yarn cq:circular-deps",
        "cq:circular-deps": "dpdm src/**/*.{ts,tsx} --circular -T --tree false --warning false --exit-code circular:1"
    },
    "dependencies": {
        "@act-aks/rn-carousel": "1.1.6",
        "@react-navigation/bottom-tabs": "7.3.14",
        "@react-navigation/elements": "2.4.3",
        "@react-navigation/native": "7.1.10",
        "@react-navigation/native-stack": "7.3.14",
        "@tanstack/react-query": "5.80.6",
        "class-variance-authority": "0.7.1",
        "clsx": "2.1.1",
        "lucide-react-native": "0.514.0",
        "nativewind": "4.1.23",
        "react": "19.1.0",
        "react-native": "0.79.3",
        "react-native-bootsplash": "6.3.8",
        "react-native-gesture-handler": "2.25.0",
        "react-native-mmkv": "3.2.0",
        "react-native-reanimated": "3.18.0",
        "react-native-safe-area-context": "5.4.1",
        "react-native-screens": "4.11.1",
        "react-native-svg": "15.12.0",
        "tailwind-merge": "3.3.1",
        "tailwindcss": "3.4.17"
    },
    "devDependencies": {
        "@act-aks/eslint-config": "1.0.6",
        "@babel/core": "7.27.4",
        "@babel/preset-env": "7.27.2",
        "@babel/runtime": "7.27.6",
        "@commitlint/cli": "19.8.1",
        "@commitlint/config-conventional": "19.8.1",
        "@eslint/js": "9.28.0",
        "@react-native-community/cli": "18.0.0",
        "@react-native-community/cli-platform-android": "18.0.0",
        "@react-native-community/cli-platform-ios": "18.0.0",
        "@react-native/babel-preset": "0.80.0",
        "@react-native/eslint-config": "0.79.3",
        "@react-native/metro-config": "0.79.3",
        "@react-native/typescript-config": "0.79.3",
        "@tanstack/eslint-plugin-query": "5.78.0",
        "@testing-library/jest-native": "5.4.3",
        "@testing-library/react": "16.3.0",
        "@testing-library/react-native": "13.2.0",
        "@trivago/prettier-plugin-sort-imports": "5.2.2",
        "@types/jest": "29.5.14",
        "@types/react": "19.1.6",
        "@types/react-test-renderer": "19.1.0",
        "babel-plugin-module-resolver": "5.0.2",
        "dpdm": "3.14.0",
        "eslint": "9.28.0",
        "globals": "16.2.0",
        "jest": "29.7.0",
        "lefthook": "1.11.13",
        "prettier": "3.5.3",
        "prettier-plugin-sort-json": "4.1.1",
        "prettier-plugin-tailwindcss": "0.6.12",
        "react-test-renderer": "19.1.0",
        "typescript": "5.8.3"
    },
    "engines": {
        "node": ">=22",
        "yarn": ">=4.8.1"
    },
    "packageManager": "yarn@4.9.2"
}
