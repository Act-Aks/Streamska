{
    "name": "Streamska",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "typecheck": "tsc --noEmit",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prettier": "prettier --check --ignore-unknown .",
        "prettier:fix": "prettier --write --ignore-unknown .",
        "start": "react-native start",
        "test": "jest",
        "preinstall": "npx only-allow yarn",
        "up:i": "yarn upgrade-interactive",
        "up:self": "yarn set version latest && yarn",
        "cq": "yarn prettier && yarn lint && yarn typecheck && yarn cq:circular-deps",
        "cq:circular-deps": "dpdm src/**/*.{ts,tsx} --circular -T --tree false --warning false --exit-code circular:1"
    },
    "dependencies": {
        "@react-navigation/elements": "^2.3.8",
        "@react-navigation/native": "^7.1.6",
        "@react-navigation/native-stack": "^7.3.10",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "nativewind": "^4.1.23",
        "react": "19.0.0",
        "react-native": "0.79.0",
        "react-native-bootsplash": "^6.3.4",
        "react-native-gesture-handler": "^2.25.0",
        "react-native-mmkv": "^3.2.0",
        "react-native-reanimated": "^3.17.3",
        "react-native-safe-area-context": "^5.3.0",
        "react-native-screens": "^4.10.0",
        "tailwind-merge": "^3.2.0",
        "tailwindcss": "^3.4.17"
    },
    "devDependencies": {
        "@act-aks/eslint-config": "^1.0.2",
        "@babel/core": "^7.26.10",
        "@babel/preset-env": "^7.26.9",
        "@babel/runtime": "^7.27.0",
        "@commitlint/cli": "^19.8.0",
        "@commitlint/config-conventional": "^19.8.0",
        "@eslint/js": "^9.24.0",
        "@react-native-community/cli": "18.0.0",
        "@react-native-community/cli-platform-android": "18.0.0",
        "@react-native-community/cli-platform-ios": "18.0.0",
        "@react-native/babel-preset": "0.79.0",
        "@react-native/eslint-config": "0.79.0",
        "@react-native/metro-config": "0.79.0",
        "@react-native/typescript-config": "0.79.0",
        "@testing-library/jest-native": "^5.4.3",
        "@testing-library/react": "^16.3.0",
        "@testing-library/react-native": "^13.2.0",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/jest": "^29.5.14",
        "@types/react": "^19.1.0",
        "@types/react-test-renderer": "^19.1.0",
        "babel-plugin-module-resolver": "^5.0.2",
        "dpdm": "^3.14.0",
        "eslint": "^9.24.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-native": "^5.0.0",
        "eslint-plugin-testing-library": "^7.1.1",
        "globals": "^16.0.0",
        "jest": "^29.7.0",
        "lefthook": "^1.11.8",
        "prettier": "^3.5.3",
        "prettier-plugin-sort-json": "^4.1.1",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "react-test-renderer": "19.0.0",
        "typescript": "5.8.3",
        "typescript-eslint": "^8.29.1"
    },
    "engines": {
        "node": ">=22",
        "yarn": ">=4.8.1"
    },
    "packageManager": "yarn@4.9.0"
}
